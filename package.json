{
  "name": "happn-cluster",
  "version": "8.1.0",
  "description": "extends happn with cluster ability",
  "main": "index.js",
  "scripts": {
    "test": "gulp lint && LOG_LEVEL=warn mocha test/test-func test/test-unit --benchmarket-off",
    "lint": "gulp lint",
    "unit": "mocha test/test-unit --benchmarket-off",
    "test-cover": "LOG_LEVEL=warn istanbul cover _mocha -- test/test-func test/test-unit --benchmarket-off",
    "test-benchmarket": "node --expose-gc test/lib/serial-run",
    "release": "rm -rf node_modules && rm -f package-lock.json && npm install && npm run test-cover"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/happner/happn-cluster.git"
  },
  "keywords": [
    "mongo",
    "primus",
    "websockets",
    "pub-sub",
    "swim",
    "cluster",
    "happn"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/happner/happn-cluster/issues"
  },
  "homepage": "https://github.com/happner/happn-cluster#readme",
  "devDependencies": {
    "async": "^2.1.4",
    "coveralls": "^2.11.13",
    "expect.js": "^0.3.1",
    "gulp": "^3.9.1",
    "gulp-jshint": "^2.1.0",
    "happn-random-activity-generator": "0.2.1",
    "happn-service-mongo-2": "^2.0.0",
    "happner-serial-mocha": "1.3.0",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.5",
    "mocha": "^3.5.0",
    "mongodb": "^2.2.10",
    "request": "^2.81.0"
  },
  "dependencies": {
    "bluebird": "3.4.6",
    "clone": "1.0.2",
    "dface": "1.0.1",
    "happn-3": "^10.0.0",
    "happn-stats": "^1.0.6",
    "happn-swim": "^1.0.3",
    "http-proxy": "1.15.2",
    "sillyname": "0.1.0"
  }
}
